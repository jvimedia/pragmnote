var net_typ = function net_typ(){


    //this.hashlength = 32;				//Stringlänge des verwendeten HashCodes
    this.hashlength = 6;				//Stringlänge des verwendeten HashCodes
    
    this.hashCode = function(e){
    	//var ha = MD5UTF8(e);
    	var ha = hash.generate(e);
        return ha;
    }
};

var net = new net_typ();

